<br>
<h1>Products</h1>
<table>
    <tr>
        <th>name</th>
        <th>price</th>
        <th>stok</th>
    </tr>
    <% @product.each do |product| %>
    <tr>
        <td><%=product.name %></td>
        <td><%=product.price %></td>
        <td><%=product.stok %></td>
    </tr>
    <% end %>
</table>